{
  "name": "dot-store",
  "version": "0.3.0",
  "description": "Like Redux, but easy",
  "keywords": [
    "accessor",
    "fs",
    "immutable",
    "json",
    "redux",
    "store"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/dot-store.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/invrs/dot-store#readme",
  "scripts": {
    "build": "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "6",
    "babel-core": "6",
    "babel-eslint": "8",
    "babel-plugin-transform-async-to-generator": "6",
    "babel-plugin-transform-object-rest-spread": "6",
    "babel-preset-node6": "11",
    "fxtr": "1",
    "jest": "22",
    "rimraf": "2"
  },
  "peerDependencies": {
    "camel-dot-prop-immutable": "1"
  },
  "babel": {
    "presets": [
      "node6"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/test/"
    ],
    "setupFiles": [
      "./test/setup.js"
    ],
    "testMatch": [
      "**/*Test.js"
    ],
    "verbose": true
  }
}
