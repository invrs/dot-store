{
  "name": "dot-store-fs",
  "version": "2.6.0",
  "description": "Adds filesystem read/write to dot-store",
  "keywords": [
    "accessor",
    "fs",
    "immutable",
    "json",
    "store"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/dot-store.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/invrs/dot-store/tree/master/packages/dot-store-fs#readme",
  "scripts": {
    "build": "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest",
    "watch": "rimraf dist; babel lib --watch --out-dir dist --source-maps"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.42",
    "@invrs/dot-prop-immutable": "^2.6.0",
    "fs-extra": "5",
    "glob": "7",
    "proper-lockfile": "3",
    "rimraf": "2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "fxtr": "1",
    "jest": "22",
    "rimraf": "2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.14"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*Test.js"
    ],
    "verbose": true
  },
  "starters": [
    "babel",
    "babel-node",
    "babel-scripts",
    "basics",
    "jest",
    "watchman-babel"
  ],
  "watchman": {
    "triggers": [
      {
        "name": "babel",
        "expression": [
          "anyof",
          [
            "match",
            "lib/**/*.js",
            "wholename"
          ]
        ],
        "command": [
          "npm",
          "run",
          "build"
        ]
      }
    ]
  }
}
