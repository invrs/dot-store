{
  "name": "dot-store-fs",
  "version": "2.4.1",
  "description": "Adds filesystem read/write to dot-store",
  "keywords": [
    "accessor",
    "fs",
    "immutable",
    "json",
    "store"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/dot-store.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage":
    "https://github.com/invrs/dot-store/tree/master/packages/dot-store-fs#readme",
  "scripts": {
    "build":
      "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest test"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.47",
    "dot-prop-immutable": "^1.4.0",
    "fs-extra": "5",
    "glob": "7",
    "proper-lockfile": "3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread":
      "^7.0.0-beta.47",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "babel-core": "^7.0.0-bridge.0",
    "fxtr": "1",
    "jest": "^22.4.3",
    "rimraf": "2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "node": "current"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "setupFiles": ["./test/setup.js"],
    "testMatch": ["**/*Test.js"],
    "verbose": true
  }
}
