{
  "name": "dot-store-message",
  "version": "2.4.3",
  "description": "Store sync using window.postMessage",
  "keywords": [
    "accessor",
    "immutable",
    "json",
    "postMessage",
    "store"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/dot-store.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/invrs/dot-store/tree/master/packages/dot-store-message#readme",
  "scripts": {
    "build": "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest",
    "watch": "rimraf dist; babel lib --watch --out-dir dist --source-maps"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.42",
    "rimraf": "2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "fxtr": "1",
    "jest": "22",
    "rimraf": "2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 0.5%",
              "last 2 versions",
              "Firefox ESR",
              "not dead"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*Test.js"
    ],
    "verbose": true
  },
  "starters": [
    "basics",
    "babel",
    "babel-scripts",
    "babel-web",
    "jest"
  ]
}
