{
  "name": "@invrs/dot-prop-immutable",
  "version": "2.4.4",
  "description": "Immutable version of dot-prop with some extensions",
  "keywords": [
    "immutable",
    "dot-prop",
    "react",
    "redux",
    "obj",
    "object",
    "prop",
    "property",
    "dot",
    "path",
    "get",
    "access",
    "notation"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/dot-store.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/invrs/dot-store#readme",
  "scripts": {
    "build": "rimraf dist; babel index.js --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.42"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "emittery": "^0.3.0",
    "fxtr": "1",
    "jest": "22",
    "rimraf": "2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 0.5%",
              "last 2 versions",
              "Firefox ESR",
              "not dead"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/test/"
    ],
    "setupFiles": [
      "./test/setup.js"
    ],
    "testMatch": [
      "**/*.spec.js"
    ],
    "verbose": true
  }
}
