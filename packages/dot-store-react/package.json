{
  "name": "dot-start-react",
  "version": "0.0.1",
  "description": "Node.js starter project",
  "keywords": ["boilerplate", "nodejs", "starter"],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/node-starter.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage":
    "https://github.com/invrs/node-starter#readme",
  "scripts": {
    "build": "babel lib --out-dir dist",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest test"
  },
  "devDependencies": {
    "babel-cli": "6",
    "babel-core": "6",
    "babel-eslint": "8",
    "babel-jest": "22",
    "babel-plugin-transform-async-to-generator": "6",
    "babel-plugin-transform-object-rest-spread": "6",
    "babel-preset-env": "1",
    "babel-preset-node6": "11",
    "babel-preset-react": "6",
    "fxtr": "1",
    "jest": "22",
    "react-test-renderer": "16"
  },
  "dependencies": {
    "create-react-context": "^0.2.1",
    "react": "16",
    "react-dom": "16"
  },
  "babel": {
    "presets": ["env", "react"],
    "plugins": [
      "transform-async-to-generator",
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": ["./test/setup.js"],
    "testMatch": ["**/*Test.js"]
  }
}
