{
  "name": "dot-store-react",
  "version": "2.4.8",
  "description": "React integration for dot-store",
  "keywords": [
    "accessor",
    "immutable",
    "react",
    "redux",
    "store"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/dot-store.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/invrs/dot-store/tree/master/packages/dot-store-react#readme",
  "scripts": {
    "build": "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest",
    "watch": "rimraf dist; babel lib --watch --out-dir dist --source-maps"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.42",
    "rimraf": "2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-react": "7.0.0-beta.42",
    "@invrs/enzyme": "^3.3.0",
    "@invrs/enzyme-adapter-react-16": "^1.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "fxtr": "1",
    "jest": "22",
    "react": "^16.3.2",
    "react-dom": "16",
    "rimraf": "2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 0.5%",
              "last 2 versions",
              "Firefox ESR",
              "not dead"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*Test.js"
    ],
    "verbose": true
  },
  "starters": [
    "babel",
    "babel-scripts",
    "babel-web",
    "babel-web-react",
    "basics",
    "jest",
    "jest-react",
    "watchman-babel"
  ],
  "watchman": {
    "triggers": [
      {
        "name": "babel",
        "expression": [
          "anyof",
          [
            "match",
            "lib/**/*.js",
            "wholename"
          ]
        ],
        "command": [
          "npm",
          "run",
          "build"
        ]
      }
    ]
  }
}
