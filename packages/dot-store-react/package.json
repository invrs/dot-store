{
  "name": "dot-store-react",
  "version": "2.1.0",
  "description": "React integration for dot-store",
  "keywords": [
    "accessor",
    "immutable",
    "react",
    "redux",
    "store"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/dot-store.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage":
    "https://github.com/invrs/dot-store/tree/master/packages/dot-store-react#readme",
  "scripts": {
    "build":
      "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest test"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.47",
    "create-react-context": "0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread":
      "^7.0.0-beta.47",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-react": "7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "enzyme": "3",
    "enzyme-adapter-react-16": "1",
    "fxtr": "1",
    "jest": "^22.4.3",
    "react": "16.1",
    "react-dom": "16",
    "rimraf": "2"
  },
  "babel": {
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "setupFiles": ["./test/setup.js"],
    "testMatch": ["**/*Test.js"],
    "verbose": true
  }
}
