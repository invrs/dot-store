{
  "name": "dot-store-react",
  "version": "0.5.0",
  "description": "React integration for dot-store",
  "keywords": [
    "accessor",
    "immutable",
    "react",
    "redux",
    "store"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/dot-store.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage":
    "https://github.com/invrs/dot-store/tree/master/packages/dot-store-react#readme",
  "scripts": {
    "build":
      "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest test"
  },
  "devDependencies": {
    "babel-cli": "6",
    "babel-core": "6",
    "babel-eslint": "8",
    "babel-jest": "22",
    "babel-plugin-transform-async-to-generator": "6",
    "babel-plugin-transform-object-rest-spread": "6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6",
    "enzyme": "3",
    "enzyme-adapter-react-16": "1",
    "fxtr": "1",
    "jest": "22",
    "rimraf": "2"
  },
  "peerDependencies": {
    "create-react-context": "0.2",
    "react": "16",
    "react-dom": "16"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": ["last 2 versions"],
            "useBuiltIns": true
          }
        }
      ],
      "react"
    ]
  },
  "jest": {
    "setupFiles": ["./test/setup.js"],
    "testMatch": ["**/*Test.js"],
    "verbose": true
  }
}
