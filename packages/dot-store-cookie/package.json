{
  "name": "dot-store-cookie",
  "version": "2.6.25",
  "description": "Cookie integration for dot-store",
  "keywords": [
    "dot-store",
    "cookie"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/arch.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/invrs/arch/tree/master/packages/dot-store-cookie#readme",
  "scripts": {
    "build": "babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "rimraf dist; npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest",
    "watch": "rimraf dist; babel lib --watch --out-dir dist --source-maps"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.42",
    "js-cookie": "^2.2.0",
    "rimraf": "2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "fxtr": "1",
    "jest": "23.4.2",
    "rimraf": "^2.6.2",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.42"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 0.5%",
              "last 2 versions",
              "Firefox ESR",
              "not dead"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*Test.js"
    ],
    "testURL": "http://localhost"
  },
  "starters": [
    "babel",
    "babel-scripts",
    "babel-web",
    "basics",
    "jest",
    "watchman-babel"
  ],
  "watchman": {
    "triggers": [
      {
        "name": "babel",
        "expression": [
          "anyof",
          [
            "match",
            "lib/**/*.js",
            "wholename"
          ]
        ],
        "command": [
          "npm",
          "run",
          "build"
        ]
      }
    ]
  }
}
