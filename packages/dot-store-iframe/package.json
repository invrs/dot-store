{
  "name": "dot-store-iframe",
  "version": "2.4.6",
  "description": "Iframe controller integration for dot-store",
  "keywords": [
    "iframe",
    "dot-store",
    "dfp"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/arch.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/invrs/arch/tree/master/packages/dot-store-iframe#readme",
  "scripts": {
    "build": "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest test",
    "watch": "rimraf dist; babel lib --watch --out-dir dist --source-maps"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.42"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "dot-store": "^2.4.6",
    "fxtr": "1",
    "jest": "^22.4.3",
    "rimraf": "2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 0.5%",
              "last 2 versions",
              "Firefox ESR",
              "not dead"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testMatch": [
      "**/*Test.js"
    ]
  }
}
