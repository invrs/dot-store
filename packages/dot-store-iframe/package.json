{
  "name": "dot-store-iframe",
  "version": "0.0.1",
  "description":
    "Iframe controller integration for dot-store",
  "keywords": ["iframe", "dot-store", "dfp"],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/arch.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage":
    "https://github.com/invrs/arch/tree/master/packages/dot-store-iframe#readme",
  "scripts": {
    "build":
      "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest test",
    "watch":
      "rimraf dist; babel lib --watch --out-dir dist --source-maps"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "dot-store": "^2.2.2",
    "fxtr": "1",
    "jest": "^22.4.3",
    "rimraf": "2"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "jest": {
    "setupFiles": ["./test/setup.js"],
    "testMatch": ["**/*Test.js"]
  }
}
